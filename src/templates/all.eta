#pragma once
#include <map>
#include <string>
#include <saucer/webview.hpp>

<% it.files.forEach(function(file){ %>
#include "<%= file.path %>.hpp"
<% }) %>

namespace saucer::embedded
{
    inline auto all()
    {
        std::map<std::string, embedded_file> rtn;

        <% it.files.forEach(function(file){ %>
        rtn.emplace("<%= file.path %>", embedded_file{"<%= file.mime %>", <%= file.name %>);
        <% }) %>

        return rtn;
    }
} // namespace saucer::embedded
